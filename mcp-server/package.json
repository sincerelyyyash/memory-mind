{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for AI chat memory management",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot index.ts",
    "start": "bun run index.ts",
    "build": "bun build index.ts --outdir ./dist",
    "clean": "rm -rf dist",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:migrate:reset": "bunx prisma migrate reset",
    "db:studio": "bunx prisma studio",
    "db:push": "bunx prisma db push",
    "db:init": "bunx prisma migrate dev --name init",
    "test": "echo \"Tests not implemented yet\"",
    "lint": "echo \"Linting not configured yet\"",
    "postinstall": "bunx prisma generate"
  },
  "keywords": ["mcp", "memory", "context", "ai", "chat", "sqlite", "express"],
  "author": "Townsquare Assignment",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "prisma": "^6.12.0",
    "@prisma/client": "^6.12.0",
    "zod": "^3.22.4"
  }
}
